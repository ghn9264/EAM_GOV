@model EAM.Data.Domain.LandAttribute
@{
    ViewBag.Title = "条目输入界面";
    Layout = "~/Areas/AssetsInput/Views/AssetsInput/Input.cshtml";
}

<!--- 路径显示 -->
<script type="text/javascript">
    $(document).ready(function () {
        $("#navTitle").empty();
        $("#navTitle").append('<ol class="breadcrumb"> <li><a href="@Url.Action("Index", "DeskTop", new { Area = "Account" })" style="font-size: 18px"><i class="fa fa-dashboard"></i>首页</a></li> <li><a href="#" style="font-size: 18px">资产登记</a></li> <li><a href="#" style="font-size: 18px">土地</a></li></ol>');
        //alert("YES");
    });
</script>


<!-- 土地资产差异化属性 -->
<div class="col-lg-6">
    <div class="box box-info">
        <!-- Header -->
        <div class="box-header with-border">
            <h3 class="box-title" style="font-size: 18px;font-family: '微软雅黑';color: rgb(18, 136, 198);">土地信息</h3>
            <div class="box-tools pull-right">
                <button class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
            </div>
        </div>
        <!-- Body -->
        <div class="box-body">
            <div class="box-body">
                <!-- 产权形式 -->
                <div class="form-group">
                    <label>产权形式</label>
                    <select class="form-control select2 select2-hidden-accessible" style="width: 100%;" id="PropertyForm" name="PropertyForm">
                        <option selected="selected">有产权</option>
                        <option>无产权</option>
                        <option>产权待界定</option>
                    </select>
                </div>

                <!-- 权属证明 -->
                <div class="form-group">
                    <label>权属证明</label>
                    <input type="text" class="form-control" id="OwnershipCertificate" name="OwnershipCertificate" placeholder="权属证明" value="@Model.OwnershipCertificate">
                </div>

                <!-- 权属性质 -->
                <div class="form-group">
                    <label>权属性质</label>
                    <select class="form-control select2 select2-hidden-accessible" style="width: 100%;" id="OwnershipStyle" name="OwnershipStyle">
                        <option selected="selected">国有</option>
                        <option>集体</option>
                    </select>
                </div>

                <!-- 权属年限 -->
                <div class="form-group">
                    <label>权属年限</label>
                    <input type="text" class="form-control" id="OwnershipTime" name="OwnershipTime" placeholder="权属年限" value="@Model.OwnershipTime">
                </div>

                <!-- 权属证号 -->
                <div class="form-group">
                    <label>权属证号</label>
                    <input type="text" class="form-control" id="OwnershipCertifiateNum" name="OwnershipCertifiateNum" placeholder="权属证号" value="@Model.OwnershipCertifiateNum">
                </div>

                <!-- 发证日期 -->
                <div class="form-group">
                    <label>发证日期</label>
                    <div class="input-group">
                        <div class="input-group-addon">
                            <i class="fa fa-calendar"></i>
                        </div>
                        <input type="text" class="form-control " data-provide="datepicker" data-date-format="yyyy-mm-dd"
                               id="IssuingDate" name="IssuingDate" value="@Model.IssuingDate">
                    </div>
                </div>

            </div><!-- /.box-body -->
        </div><!-- /.box-body -->
    </div>
</div>

<!-- 土地资产差异化属性 -->
<div class="col-lg-6">
    <div class="box box-info">
        <!-- Header -->
        <div class="box-header with-border">
            <h3 class="box-title" style="font-size: 18px;font-family: '微软雅黑';color: rgb(18, 136, 198);">土地信息</h3>
            <div class="box-tools pull-right">
                <button class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
            </div>
        </div>
        <!-- Body -->
        <div class="box-body">
            <div class="box-body">

                <!-- 土地使用权人 -->
                <div class="form-group">
                    <label>土地使用权人</label>
                    <input type="text" class="form-control" id="LandOwner" name="LandOwner" placeholder="土地使用权人" value="@Model.LandOwner">
                </div>

                <!-- 土地使用权类型 -->
                <div class="form-group">
                    <label>土地使用权类型</label>
                    <select class="form-control select2 select2-hidden-accessible" style="width: 100%;"  id="LandUserStyle" name="LandUserStyle">
                        <option selected="selected">出让</option>
                        <option>划拨</option>
                        <option>租凭</option>
                    </select>
                </div>

                <!-- 坐落位置 -->
                <div class="form-group">
                    <label>坐落位置</label>
                    <input type="text" class="form-control" id="Location" name="Location" placeholder="坐落位置" value="@Model.Location">
                </div>

                <!-- 土地使用权面积 -->
                <div class="form-group">
                    <label>土地使用权面积</label>
                    <input type="text" class="form-control" id="AllArea" name="AllArea" placeholder="土地使用权面积"
                           onfocus="NumOnfocus('AllArea')" onblur="isNum('AllArea')" value="@Model.AllArea">
                </div>

                <!-- 分摊面积 -->
                <div class="form-group">
                    <label>分摊面积</label>
                    <input type="text" class="form-control" id="SharingArea" name="SharingArea" placeholder="分摊面积"
                           onfocus="NumOnfocus('SharingArea')" onblur="isNum('SharingArea')" value="@Model.SharingArea">
                </div>

                <!-- 独用面积 -->
                <div class="form-group">
                    <label>独用面积</label>
                    <input type="text" class="form-control" id="SingleArea" name="SingleArea" placeholder="独用面积"
                            onfocus="NumOnfocus('SingleArea')" onblur="isNum('SingleArea')" value="@Model.SharingArea">
                </div>

                <!-- 入账科目 -->
                <div class="form-group">
                    <label>入账科目</label>
                    <select class="form-control select2 select2-hidden-accessible" style="width: 100%;">
                        <option selected="selected">固定资产</option>
                        <option>无形资产</option>
                    </select>
                </div>

            </div><!-- /.box-body -->

        </div><!-- /.box-body -->
    </div>
</div>
<input type="hidden" name="AttrId" value="@Model.AttrId">
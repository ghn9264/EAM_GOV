@model EAM.Data.Domain.BuildingAttribute
@{
    ViewBag.Title = "条目输入界面";
    Layout = "~/Areas/AssetsInput/Views/AssetsInput/Input.cshtml";
}

<!--- 路径显示 -->
<script type="text/javascript">
    $(document).ready(function () {
        $("#navTitle").empty();
        $("#navTitle").append('<ol class="breadcrumb"> <li><a href="#@Url.Action("Index", "DeskTop", new { Area = "Account" })" style="font-size: 18px"><i class="fa fa-dashboard"></i>首页</a></li> <li><a href="#" style="font-size: 18px">资产登记</a></li> <li><a href="#" style="font-size: 18px">构筑物</a></li></ol>');
        //alert("YES");
    });
</script>

<!-- 构筑物资产差异化属性 -->
<div class="col-lg-12">
    <div class="box box-info">
        <!-- Header -->
        <div class="box-header with-border">
            <h3 class="box-title" style="font-size: 18px;font-family: '微软雅黑';color: rgb(18, 136, 198);">构筑物信息</h3>
            <div class="box-tools pull-right">
                <button class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
            </div>
        </div>
        <!-- Body -->
        <div class="box-body">
            <div class="box-body">
                <!-- 产权形式 -->
                <div class="form-group">
                    <label>产权形式</label>
                    <select class="form-control select2 select2-hidden-accessible" style="width: 100%;" id="PropertyForm" name="PropertyForm">
                        <option selected="selected">产权在本单位（有证）</option>
                        <option>产权在本单位（无证）</option>
                        <option>产权不在单位</option>
                        <option>产权带界定</option>
                        <option>其他</option>
                    </select>
                </div>

                <!-- 权属证号 -->
                <div class="form-group">
                    <label>权属证号</label>
                    <input type="text" class="form-control" id="OwnershipCertifiateNum" name="OwnershipCertifiateNum" placeholder="权属证号" value="@Model.OwnershipCertifiateNum">
                </div>

                <!-- 建筑物面积 -->
                <div class="form-group">
                    <label>构筑物面积</label>
                    <input type="text" class="form-control" id="Area" name="Area" placeholder="建筑物面积" onblur="isNumEx('Area')" onfocus="NumOnfocus('Area')" value="@Model.Area">
                </div>

                <!-- 坐落位置 -->
                <div class="form-group">
                    <label>坐落位置</label>
                    <input type="text" class="form-control" id="Location" name="Location" placeholder="坐落位置" value="@Model.Location">
                </div>

            </div><!-- /.box-body -->
         
        </div><!-- /.box-body -->
    </div>
</div>
<input type="hidden" name="AttrId" value="@Model.AttrId">
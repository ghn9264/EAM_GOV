<!-- 数据批量导入 -->
<section class="content">
    <!-- 数据批量导入操作面板 -->
    <div class="col-lg-6">

        <!-- 数据批量导入操作面板 -->
        <div class="box box-danger">

            <!-- 表头 -->
            <div class="box-header with-border">
                <h3 class="box-title" style="font-size: 18px;font-family: '微软雅黑';color: rgb(18, 136, 198);">数据批量导入</h3>
                <div class="box-tools pull-right">
                    <button class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
                </div>
            </div><!-- /.box-header -->
            <!-- body -->
            <div class="box-body content">
                <div class="row">
                    <!-- 模板下载 -->
                    <div class="box-body col-lg-6">
                        <!-- 模板下载 -->
                        <div class="form-group col-lg-offset-5 with-border">
                            <br />
                            <a class="btn btn-app">
                                <span class="badge bg-aqua"></span>
                                <i class="fa fa-envelope"></i> 模板文件
                            </a>
                        </div>
                    </div>

                    <!-- 数据上传 -->
                    <div class="box-body col-lg-6">
                        <!-- 文件上传 -->
                        <br />
                        <br />
                        <input type="file" id="exampleInputFile">
                    </div>
                </div>

                <div class="row">
                    <div class="col-lg-6">
                        <button type="submit" class="btn btn-primary col-lg-offset-6">下载</button>
                    </div>
                    <div class="col-lg-6">
                        <button type="submit" class="btn btn-primary col-lg-offset-1">上传</button>
                    </div>
                </div>
            </div>


        </div><!--/.box -->
        <!-- 图片批量导入操作面板 -->
        <div class="box box-danger">

            <!-- 表头 -->
            <div class="box-header with-border">
                <h3 class="box-title" style="font-size: 18px;font-family: '微软雅黑';color: rgb(18, 136, 198);">图片批量导入</h3>
                <div class="box-tools pull-right">
                    <button class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
                </div>
            </div><!-- /.box-header -->

            <div class="box-body content">
                <!-- body -->
                <form method="post" enctype="multipart/form-data">
                    <div class="row">
                        <div class="box-body col-lg-6">
                            <div class="form-group col-lg-offset-0 with-border">
                                <br />
                                <text>
                                    上传说明：
                                    xxxx
                                </text>
                            </div>
                        </div>

                        <!-- 数据上传 -->
                        <div class="box-body col-lg-6">
                            <!-- 文件上传 -->
                            <br />
                            <br />
                            <input type="file" id="file" name="file">
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-lg-6">
                            @*<button type="submit" class="btn btn-primary col-lg-offset-6">下载</button>*@
                        </div>
                        <div class="col-lg-6">
                            <br />
                            <br />
                            <button type="submit" class="btn btn-primary col-lg-offset-1">上传</button>
                        </div>
                    </div>
                </form>
            </div>
        </div><!--/.box -->
    </div>


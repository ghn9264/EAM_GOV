        //
        var _code = classcode.substring(0, 3);

        //
        // 资产十大类分类代码
        //
        var land = ["101"];
        var house = ["102"];
        var build = ["103"];
        var car = ["203"];
        var general = ["201", "202", "204", "210", "220", "230", "231", "232", "240", "241", "242"];

<script type="text/javascript">
    var selectPlaceNode = null;
    var gallery;
    var table1;
    var table2;
    var table3;
    var table4;
    var table5;

    $(document).ready(function () {

        //document.getElementById("IndexPanel").style.display = ""; //显示
        //document.getElementById("TreeIndexPanel").style.display = ""; //显示

        //
        // 为筛选资产按钮添加函数
        //
    });

    var scrapQueryUrl = "@Url.Action("AllScrapRecordQuery", "AssetsFind", new { area = "AssetsFind" })";

    function queryScrapRecordData(pageIndex) {
        $("#ScrapRecordPageIndex").val(pageIndex);
        //var d = $("#NoneFrm").serialize();
        $.post(scrapQueryUrl, function (result) {
            $("#ScrapRecordTable tbody").empty();
            $(result.Items).each(function (index, item) {
                var row = $(" <tr role='row'>");
                row.append('<td><span id="entityID">' + item.EntityId + '</span></td>');
                row.append('<td>' + item.ScrapPerson + '</td>');
                row.append('<td>' + item.ScrapUnit + '</td>');
                row.append('<td>' + item.ScrapDate + '</td>');
                if (item.HasScrap == 0)
                    row.append('<td>未报废</td>');
                else
                    row.append('<td>已报废</td>');

                row.append('<td>' + item.ScrapExaminePerson + '</td>');
                row.append('<td>' + item.ScrapExamineDate + '</td>');

                if (item.HasScrap == 0)
                    row.append('<td><span class="btn btn-default btn-all scrapreply" data-id=' + item.EntityId + '>分类合并</span></td>');
                else
                    row.append('<td>&nbsp;</td>');
                $("#ScrapRecordTable").append(row);
            });

            //
            // 分类合并按钮事件
            //
            var scrapreply = "@Url.Action("DoPreScrapReply", "AssetsFind", new { area = "AssetsFind" })";

            $(".scrapreply").click(function () {

                var entity = $(this).attr("data-id");
                $("#Biaodanhao").val(entity);
                $.post(scrapreply, { "entity": entity }, function (result) {
                    //
                    // 如果分类合并成功则请求数据显示
                    //
                    if (result == "OK") {
                        QueryMerge(entity);


                    } else {
                        alert("分类合并失败，原因：" + result);
                    }

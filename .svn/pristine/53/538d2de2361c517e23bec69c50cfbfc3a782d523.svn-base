    //
    // 将数据显示到导出列表
    //
    function AddGetOutTable() {
        var getOut = "@Url.Action("GetOut", "DoAssets", new { area = "AssetsFind" })";
        $.post(getOut, function (result) {
            if (result == null) {
                alert("导出列表数据查询失败，原因：" + result);
            } else {
                //
                // 清空列表内容
                //
                if (table5 != null) {
                    table5.destroy();
                }
                $("#tableGetOut tbody").empty();
                $(result.Items).each(function (index, item) {
                    var row = $(" <tr role='row'>");
                    row.append('<td><input type="checkbox" name="checkbox" del-id="' + item.EntityId + '" /></td>');
                    row.append('<td>' + item.AssetsNum + '</td>');
                    row.append('<td>' + item.AssetsName + '</td>');
                    row.append('<td>' + item.GetsDate + '</td>');
                    row.append('<td>' + item.FillPerson + '</td>');
                    switch (item.HasGetOut) {
                        case 0:
                            row.append('<td>未批复</td>');
                            break;
                        case 1:
                            row.append('<td>已导出</td>');
                            break;
                        case 2:
                            row.append('<td>已批复，未导出</td>');
                            break;
                        default:
                            break;
                    }
                    //row.append('<td><a  class="btn btn-default btn-all exportScrap" href="/AssetsFind/DoAssets/ExportSingleScrapTable?Entity=' + item.EntityId + '" >导出</a></td>');
                    //row.append('<td><input type="button"  class="btn btn-default btn-all deletbtn" del-id="' + item.EntityId + '" name="' + item.EntityId + '" value="删除" /></td>');

                    $("#tableGetOut").append(row);

                });
                table5 = $("#tableGetOut").DataTable();



                //
                // 注册删除按钮点击时间
                //
                var delet = "@Url.Action("DeletScrap", "DoAssets", new { area = "AssetsFind" })";

                $(".deletbtn").click(function () {

                    var test = $("input[name='checkbox']:checked");
                    var checkBoxValue = "";
                    test.each(function () {
                        checkBoxValue += $(this).attr("del-id") + "|";
                    });
                    if (checkBoxValue.length < 1) { alert("请选择"); } else {
                        //var entity = $(this).attr("del-id");
                        $.post(delet, { "Entity": checkBoxValue }, function (result) {
                            if (result == "OK")
                                alert("删除成功");
                        }, "json");
                    }
                });

                //
                // 注册导出按钮事件处理
                //
                $(".exportScrap").click(function () {

                    var test = $("input[name='checkbox']:checked");
                    var checkBoxValue = "";
                    test.each(function () {
                        checkBoxValue += $(this).attr("del-id") + "|";
                    })
                    if (checkBoxValue.length < 1) { alert("请选择"); } else {
                        var ExpertScrapTable = '@Url.Action("ExportSingleScrapTable", "DoAssets", new { area = "AssetsFind" })';

                        var entity = $(this).attr("exportScrap-id");

                        $.post(ExpertScrapTable, { "Entity": checkBoxValue }, function () { });
                    }
                });

            }
        });
    }







.new_content{
	padding-left:0px;
	padding-top:0px;
	
	}
.box-inventory{
	width: 100%;
	border-top: 0;
	min-height: 401px;
}
.clo-inven{
	padding-left:0 ;
}
.row-inven{
	margin-left: 0;
	margin-right: 0;
}
.new-box-header{
	padding-top: 20px;
	padding-bottom: 20px;
	padding-left: 30px;
}
.title-a{
	font-weight: bold;
	color: rgb(51,51,51);
	font-size: 20px !important;
}	
.top_a {
    color: rgb(118,118,119);
    background: white;
    border: 0;
    font-size: 16px;
}
.pull-right{
	margin-top: 10px;
}
.pull-right span{
	display: inline-block;
    position: relative;
    height: 12px;
    width: 1px;
    background: rgb(222,223,225);
}
.new-table{
	border: 0;
}
.new-table tbody tr{
	background-color: white !important;
}
.new-table tbody tr td{
	border: 0;
	text-align: center;
}
.new-table tbody tr td a{
	color:white;
}
.tabletr{
	background: url(../../img/th-bg.png) no-repeat center center;
	background-size: 101%;
	color: rgb(39,106,179);
	font-weight: bold;
}
table tr th{
	text-align: center;
	line-height: 1.42857143 !important;
}
.con-th{
	padding: 0;
	margin-left: 0;
	margin-right: 0;
}
.btn-delete-inven{
	background-color: rgb(87,102,125);
	border: 0;
	border-radius: 4px;
}
.new-control{
	display: inline-block;
	border: 1px solid rgb(234,234,234);
	background-color: rgb(244,245,249);
}
.div-inven{
	width: 332px;
	height: 36px;
	margin:0 auto;
	margin-top: 20px;
	/*border: 1px solid;*/
	padding-left: 10px;
	
}
.div-inven label{
	display: inline-block;
	width: 80px;
	text-align: right;
}
.div-inven input{
	width: 240px;
}
.content-inven{
	padding-top:15px ;
	padding-left: 0px;
	padding-right: 15px;
}
.tabletrcc{
	background: url(../../img/tr-bg-inven.PNG) no-repeat center center;
	background-size: 101%;
	color: rgb(39,106,179);
	font-weight: bold;
}
.tabletrcc th{
	border: 0 !important;
}
.new-box-body{
	padding: 0;
}
